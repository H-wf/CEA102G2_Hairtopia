<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping 
 PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
 
 <hibernate-mapping>
 	 <class name="com.notification.model.NotiVO" table="notification"> 

        <id name="notiNo" column="notiNo"> 
            <generator class="identity"/> 
        </id> 

        <property name="memNo" column="memNo"/> 
        <property name="notiText" column="notiText"/> 
        <property name="notiTime" column="notiTime" type="timestamp"/> 
        <property name="notiIsRead" column="notiIsRead"/> 

    </class> 
 </hibernate-mapping>