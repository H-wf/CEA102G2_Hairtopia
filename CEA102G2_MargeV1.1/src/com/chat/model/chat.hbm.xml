<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping 
 PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
 
 <hibernate-mapping>
 	 <class name="com.chat.model.ChatVO" table="chat"> 

        <id name="chatNo" column="chatNo"> 
            <generator class="identity"/> 
        </id> 

        <property name="chatSender" column="chatSender"/> 

        <property name="chatReceiver" column="chatReceiver"/> 
        <property name="chatText" column="chatText"/> 
        <property name="chatTime" column="chatTime"/> 
        <property name="chatIsRead" column="chatIsRead"/> 
        <property name="chatPic" column="chatPic"/> 

    </class> 
 </hibernate-mapping>